{
    "name": "icecave/skew-entities",
    "description": "Data entities for Skew messages.",
    "homepage": "https://github.com/IcecaveStudios/skew-entities",
    "license": "MIT",
    "authors": [
        {
            "name": "James Harris",
            "email": "james.harris@icecave.com.au"
        }
    ],
    "require": {
        "php": ">=5.4",
        "eloquent/enumeration": "~3",
        "icecave/collections": "~0.6"
    },
    "require-dev": {
        "eloquent/typhoon": "~0.9",
        "icecave/archer": "~0.4"
    },
    "autoload": {
        "psr-0": {
            "Icecave\\Skew\\Entities": "lib",
            "Icecave\\Skew\\Entities\\TypeCheck": "lib-typhoon"
        }
    },
    "extra": {
        "typhoon": {
            "output-path": "lib-typhoon",
            "validator-namespace": "Icecave\\Skew\\Entities\\TypeCheck",
            "use-native-callable": true
        }
    }
}
